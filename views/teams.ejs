<%- include('header.ejs') %>
    <main>
        <table>
            <thead>
                <tr>
                    <th>name</th>
                    <th>players</th>
                </tr>
            </thead>
            <tbody>
                <% for (let character of characters) { %>
                    <tr>
                        <td>
                            <div class="nameTeams">
                                <h1><%= character.teams.name %></h1>
                                <img src="<%= character.teams.logo %>" alt=""> 
                            </div>
                        </td>
                        <td><%= character.teams.founded %></td>
                        <td><a href="/teams/<%= character.id %>">view</a></td>
                    </tr>
                <% } %>
            </tbody>
        </table>
    </main>
<%- include('footer.ejs') %>

